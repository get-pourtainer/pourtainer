    "private": true,
    "lavamoat": {
        "allowScripts": {
            "@sentry/react-native>@sentry/cli": true,
            "expo-superwall": false,
            "@biomejs/biome": false,
            "@lavamoat/preinstall-always-fail": false,
            "eas-cli>@expo/logger>bunyan>dtrace-provider": true,
            "@hot-updater/sentry-plugin>@sentry/cli": true,
            "wrangler>esbuild": false,
            "wrangler>miniflare>sharp": false,
            "wrangler>workerd": false
