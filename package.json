{
    "name": "portainer-mobile",
    "version": "1.7.1",
    "description": "The mobile app for Portainer",
    "author": {
        "name": "Pourtainer",
        "url": "https://github.com/get-pourtainer/pourtainer"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/get-pourtainer/pourtainer"
    },
    "homepage": "https://github.com/get-pourtainer/pourtainer",
    "keywords": [
        "portainer",
        "docker",
        "containers",
        "kubernetes",
        "docker-swarm",
        "expo",
        "podman",
        "rancher",
        "monorepo",
        "react-native",
        "typescript",
        "biome",
        "sentry",
        "zustand",
        "eas"
    ],
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "start": "expo start --clear",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "prebuild": "expo prebuild",
        "lint": "expo lint",
        "build:android": "eas build --platform android --profile production --local",
        "prebuild:ios": "expo prebuild --platform ios --clean",
        "prebuild:android": "expo prebuild --platform android --clean",
        "api:docker": "npx openapi-typescript ./openapi/docker.yaml -o ./lib/docker/schema.d.ts",
        "api:portainer": "npx openapi-typescript ./openapi/portainer.yaml -o ./lib/portainer/schema.d.ts",
        "eas-build-post-install": "allow-scripts run"
    },
